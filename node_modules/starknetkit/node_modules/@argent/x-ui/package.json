{
  "name": "@argent/x-ui",
  "version": "1.109.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argentlabs/x-ui.git"
  },
  "license": "MIT",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./simulation": {
      "types": "./dist/simulation.d.ts",
      "import": "./dist/simulation.js",
      "default": "./dist/simulation.cjs"
    },
    "./swap": {
      "types": "./dist/swap.d.ts",
      "import": "./dist/swap.js",
      "default": "./dist/swap.cjs"
    },
    "./theme": {
      "types": "./dist/theme.d.ts",
      "import": "./dist/theme.js",
      "default": "./dist/theme.cjs"
    },
    "./tailwind": {
      "types": "./dist/tailwind.d.ts",
      "import": "./dist/tailwind.js",
      "default": "./dist/tailwind.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "default": "./dist/react.cjs"
    },
    "./icons": {
      "types": "./dist/icons.d.ts",
      "import": "./dist/icons.js",
      "default": "./dist/icons.cjs"
    },
    "./logos": {
      "types": "./dist/logos.d.ts",
      "import": "./dist/logos.js",
      "default": "./dist/logos.cjs"
    },
    "./logos-deprecated": {
      "types": "./dist/logos-deprecated.d.ts",
      "import": "./dist/logos-deprecated.js",
      "default": "./dist/logos-deprecated.cjs"
    },
    "./styles/*": {
      "default": "./dist/styles/*"
    },
    "./i18n": {
      "types": "./dist/i18n.d.ts",
      "import": "./dist/i18n.js",
      "default": "./dist/i18n.cjs"
    },
    "./providers": {
      "types": "./dist/providers.d.ts",
      "import": "./dist/providers.js",
      "default": "./dist/providers.cjs"
    }
  },
  "devDependencies": {
    "@amplitude/analytics-types": "2.8.4",
    "@chakra-ui/cli": "2.5.6",
    "@chakra-ui/storybook-addon": "5.2.5",
    "@langchain/core": "0.3.43",
    "@langchain/google-genai": "0.1.12",
    "@lokalise/node-api": "14.2.0",
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@rollup/plugin-typescript": "12.1.2",
    "@semantic-release/git": "10.0.1",
    "@storybook/addon-essentials": "8.5.3",
    "@storybook/addon-interactions": "8.5.3",
    "@storybook/addon-links": "8.5.3",
    "@storybook/blocks": "8.5.3",
    "@storybook/nextjs": "8.5.3",
    "@storybook/react": "8.5.3",
    "@storybook/test": "8.5.3",
    "@supernovaio/sdk": "2.0.33",
    "@svgr/core": "8.1.0",
    "@svgr/plugin-jsx": "8.1.0",
    "@svgr/plugin-prettier": "8.1.0",
    "@svgr/plugin-svgo": "8.1.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@types/fs-extra": "11.0.4",
    "@types/jest-image-snapshot": "6.4.0",
    "@types/lodash-es": "4.17.12",
    "@types/object-hash": "3.0.6",
    "@types/react": "18.3.18",
    "@types/react-copy-to-clipboard": "5.0.7",
    "@types/react-dom": "18.3.5",
    "@types/yargs": "17.0.33",
    "@vitejs/plugin-react": "4.3.4",
    "@typescript-eslint/eslint-plugin": "8.23.0",
    "@typescript-eslint/parser": "8.23.0",
    "@vitejs/plugin-react-swc": "3.8.0",
    "cross-fetch": "4.1.0",
    "@vitest/browser": "3.0.9",
    "@vitest/ui": "3.0.9",
    "dotenv": "16.4.7",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-storybook": "0.11.2",
    "fs-extra": "11.3.0",
    "happy-dom": "17.0.2",
    "husky": "9.1.7",
    "jest-image-snapshot": "6.4.0",
    "lint-staged": "15.4.3",
    "playwright": "1.50.1",
    "prettier": "3.5.0",
    "rollup-plugin-preserve-directives": "0.4.0",
    "semantic-release": "24.2.2",
    "storybook": "8.5.3",
    "storybook-addon-swc": "1.2.0",
    "tsx": "4.19.2",
    "yargs": "17.7.2",
    "zod-to-json-schema": "3.24.5",
    "typescript": "5.7.3",
    "url-join": "5.0.0",
    "vite": "6.1.0",
    "vite-tsconfig-paths": "5.1.4",
    "vite-plugin-dts": "4.5.0",
    "vitest": "3.0.9",
    "@argent/x-shared": "1.73.0"
  },
  "scripts": {
    "gen:theme-typings": "pnpm build && pnpm chakra-cli tokens ./dist/index.cjs",
    "gen:tokens": "tsx ./scripts/generate-tokens.ts && pnpm gen:theme-typings",
    "gen:icons": "tsx ./scripts/generate-icons.ts",
    "lint": "eslint . --cache --ext .ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "dev": "NODE_ENV=development vite build --mode development --watch",
    "build": "vite build",
    "test": "vitest run",
    "test:update": "pnpm clean:image-snapshots && vitest run --update",
    "test:watch": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "format:fix": "prettier . --ignore-unknown --write",
    "clean:image-snapshots": "tsx ./scripts/clean-image-snapshots.ts",
    "i18n:extract": "tsx ./scripts/i18n/extract-translation.ts",
    "i18n:upload": "tsx ./scripts/i18n/upload-translation.ts",
    "i18n:download": "tsx ./scripts/i18n/download-translations.ts",
    "i18n:langchain": "tsx ./scripts/i18n/langchain-localise.ts"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@scure/bip39": "^1.2.1",
    "@starknet-io/types-js": "^0.7.7",
    "@zxcvbn-ts/core": "^3.0.0",
    "@zxcvbn-ts/language-common": "^2.0.1 || ^3.0.0",
    "@zxcvbn-ts/language-en": "^2.1.0 || ^3.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "colord": "^2.9.2",
    "embla-carousel": "^8.3.0",
    "embla-carousel-react": "^8.3.0",
    "embla-carousel-wheel-gestures": "^8.0.1",
    "emittery": "^1.0.1",
    "framer-motion": "^11.0.5",
    "history": "^5.3.0",
    "i18next": "24.2.3",
    "react-i18next": "15.4.1",
    "lightweight-charts": "^4.2.0",
    "lodash-es": "^4.17.21",
    "object-hash": "^3.0.0",
    "popmotion": "^11.0.5",
    "react": "^18.0.0",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.1 || ^7.0.0",
    "react-spring-bottom-sheet": "3.5.0-alpha.0",
    "react-virtuoso": "^4.7.2",
    "starknet": "7.5.1",
    "swr": "^1.3.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "url-join": "5.0.0",
    "zod": "4.0.5"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "vitest related --run"
    ]
  }
}
